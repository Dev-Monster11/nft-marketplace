{
  "name": "@stripe/checkout",
  "version": "2.0.0",
  "description": "Full-stack TypeScript example using Next.js, react-stripe-js, and stripe-node.",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": "./dist/lib",
    "./dist/*": "./dist/*.js",
    "./styles.css": "./dist/lib/styles.css"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "run-s build-ts build-css",
    "build-ts": "tsc",
    "build-css": "less-watch-compiler src/ dist/lib/ --run-once",
    "start": "npm-run-all --parallel watch watch-css watch-css-src",
    "watch-css": "less-watch-compiler src/ dist/lib/",
    "watch-css-src": "less-watch-compiler src/ src/",
    "watch": "tsc --watch",
    "test": "jest test --passWithNoTests",
    "clean": "rm -rf dist",
    "prepare": "run-s clean build",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|less|md)\""
  },
  "dependencies": {
    "@stripe/react-stripe-js": "1.1.2",
    "@stripe/stripe-js": "1.5.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^11.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "stripe": "8.56.0",
    "swr": "^0.1.16",
    "use-shopping-cart": "2.1.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/micro": "^7.3.3",
    "@types/micro-cors": "^0.1.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
